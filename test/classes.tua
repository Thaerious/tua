class FooBar
    x, y, z;    

    constructor(a, b)
        self.x = a + b;
    end

    foo()
        print("foo");
    end
end

class Snafu extends FooBar
    constructor(a, b)
        super:constructor(a, b);
        self.x = self.x * 2;
    end

    bar()
        print(super:foo())
    end
end

a = Snafu:new(3, 4);
print(a.x);
a.bar();